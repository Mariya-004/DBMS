mysql> select Emp_name as 'Employee',Salary as 'Monthly_Salary' from emp where Salary between 5000 and 12000 and DepNo=2 or DepNo=4;
+----------+----------------+
| Employee | Monthly_Salary |
+----------+----------------+
| Mike     |           5500 |
| Emily    |          12000 |
+----------+----------------+
2 rows in set (0.00 sec)

mysql> select Emp_name,HiredDate from emp where year(HiredDate)='1994';
